set term postscript enhanced color solid eps
set size 0.6,0.6

set xlabel "log_{10}({/Symbol l}) (meters)"
set ylabel "log_{10}(|{/Symbol a}|)"

#Homemade script for explicitly making xtics logged properly.
load 'logtics.gpl'
set ytics 1

