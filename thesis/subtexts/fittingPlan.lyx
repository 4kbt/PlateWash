\begin_layout Standard
The fitting plan:
\end_layout

\begin_layout Enumerate
Define a fitting function, 
\begin_inset Formula $F(x,B_{1\dots n}|\alpha_{1\ldots n},\lambda_{1\ldots n})=\sum_{i}\alpha_{i}B_{i}e^{-x/\lambda_{i}}+Cx$
\end_inset

.
 
\begin_inset Formula $G(x,y\dots)=F(x,\dots)-F(y,\dots)$
\end_inset


\end_layout

\begin_layout Enumerate
Measure 
\begin_inset Formula $\Delta\tau(x_{A},x_{B},B_{1},\ldots,B_{n},\sigma_{x_{A}},\sigma_{x_{B}},\sigma_{B_{1}},\ldots,\sigma_{B_{n}})$
\end_inset

.
\end_layout

\begin_layout Enumerate
Define 
\begin_inset Formula $P(a,\bar{a},\sigma_{a})=\frac{1}{\sigma_{a}\sqrt{2\pi}}e^{-(a-\bar{a})^{2}/2\sigma_{a}^{2}}$
\end_inset


\end_layout

\begin_layout Enumerate
Compute 
\begin_inset Formula $\bar{G}=\int_{-\infty}^{\infty}G(x,y,B_{1\ldots n},\alpha_{1\ldots n},\lambda_{1\ldots n})P(x,\bar{x},\sigma_{x})dxP(y,\bar{y},\sigma_{y})dy\prod_{i}P(B_{i},\bar{B}_{i},\sigma_{i})dB_{i}$
\end_inset


\end_layout

\begin_layout Enumerate
Compute 
\begin_inset Formula $\sigma_{G}^{2}=\int_{-\infty}^{\infty}(G(x,y,B_{1\ldots n},\alpha_{1\ldots n},\lambda_{1\ldots n})-\bar{G})^{2}P(x,\bar{x},\sigma_{x})dxP(y,\bar{y},\sigma_{y})dy\prod_{i}P(B_{i},\bar{B}_{i},\sigma_{i})dB_{i}$
\end_inset


\end_layout

\begin_layout Enumerate
Compute 
\begin_inset Formula $\chi^{2}$
\end_inset


\begin_inset Formula 
\[
\chi^{2}=\sum_{measurements}\frac{(\Delta\tau-\bar{G})^{2}}{\sigma_{\tau}^{2}+\sigma_{G}^{2}}
\]

\end_inset


\end_layout

\begin_layout Enumerate
